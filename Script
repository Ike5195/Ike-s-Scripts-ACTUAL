--Window
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Ike", "Ocean")




--Tool Scripts
local Main = Window:NewTab("Tools")
local ToolSection = Main:NewSection("Tools")


ToolSection:NewButton("Vaeb Remote Spy", "Spy tool to spy on remote functions. Press F9", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Ike5195/Ike-s-Scripts-ACTUAL/main/VaebRemoteSpy'))()
end)

ToolSection:NewButton("Hydroxide", "Spy tool to spy on remote functions. Clean UI", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Ike5195/Ike-s-Scripts-ACTUAL/main/Hydroxide'))()
end)

ToolSection:NewButton("Simple Spy", "Spy tool to spy on remote functions. Clean UI", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Ike5195/Ike-s-Scripts-ACTUAL/main/SimpleSpy'))()
end)

ToolSection:NewButton("Dark Dex V3", "Dark Dex V3. Dex viewer", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Ike5195/Ike-s-Scripts-ACTUAL/main/DarkDexV3'))()
end)

ToolSection:NewButton("Dark Dex V4", "Dark Dex V3. Dex viewer", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Ike5195/Ike-s-Scripts-ACTUAL/main/DarkDexV4'))()
end)

--Scripts
local Main = Window:NewTab("Scripts")
local ScriptSection = Main:NewSection("Scripts")


ScriptSection:NewButton("Town Script by Ike#5195", "A gui for the game Town on Roblox", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Ike5195/Ike-s-Scripts-ACTUAL/main/IkeTownScript'))()
end)
